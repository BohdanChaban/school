<% @title = t('activerecord.models.group.other') %>
<h1> <%= @title %> </h1>
<table>
  <thead>
    <tr>
      <th> <%= t('activerecord.attributes.group.number') %> </th>
      <th> <%= t('activerecord.attributes.group.parallel') %> </th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @groups.each do |group| %>
      <tr>
        <td><%= group.number %></td>
        <td><%= group.parallel %></td>
        <td><%= link_to  t('show'), group %></td>
      </tr>
    <% end %>
  </tbody>
</table>
